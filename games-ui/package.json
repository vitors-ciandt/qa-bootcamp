{
  "name": "ditial-products-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:wip": "cypress open -c baseUrl=http://localhost:4200 --env failOnSnapshotDiff=false -e TAGS=\"@wip\"",
    "cypress:open": "cypress open -c baseUrl=http://localhost:4200 --env failOnSnapshotDiff=false",
    "cypress:component:parallel": "npm run env -- cypress run --browser chrome --headless -s \"cypress/scenarios/component/${NAMEFOLDER}\"  -c baseUrl=http://localhost:4200 --reporter junit --reporter-options \"mochaFile=cypress/results/component-junit-report-[hash].xml\"",
    "cypress:component": "cypress run --browser chrome --headless -s \"cypress/scenarios/component/**/**\" -c baseUrl=http://localhost:4200 --reporter junit --reporter-options \"mochaFile=cypress/results/component-junit-report-[hash].xml\"",
    "cypress:component:updateSnapshots": "cypress run --browser chrome --headless -s \"cypress/scenarios/component/**/**\" -c baseUrl=http://localhost:4200 --reporter junit --reporter-options \"mochaFile=cypress/results/component-junit-report-[hash].xml\"  --env updateSnapshots=true",
    "start-playground": "cd ../ihm-micro-ui-template && npm run start-playground"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/core": "~0.7.5",
