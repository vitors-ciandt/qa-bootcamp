    "@angular/router": "6.1.10",
    "@types/file-saver": "^2.0.0",
    "aws-sdk": "^2.661.0",
    "core-js": "^2.5.4",
    "file-saver": "^1.3.8",
    "lodash": "^4.17.5",
    "moment": "^2.22.1",
    "ng-lottie": "^0.3.2",
    "node-sass": "4.14.1",
    "rxjs": "~6.2.0",
    "rxjs-compat": "^6.5.4",
